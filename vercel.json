{
  "version": 2,
  "buildCommand": "chmod +x build.sh && ./build.sh",
  "outputDirectory": "DataPilot/DataPilot/.next",
  "installCommand": "echo 'Using custom build script'",
  "framework": "nextjs",
  "functions": {
    "DataPilot/DataPilot/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/DataPilot/DataPilot/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}