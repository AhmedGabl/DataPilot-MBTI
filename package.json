{
  "name": "datapilot-mbti",
  "version": "1.0.0",
  "description": "A comprehensive MBTI personality assessment application built with Next.js, TypeScript, and Tailwind CSS",
  "main": "DataPilot/DataPilot/next.config.js",
  "scripts": {
    "dev": "cd DataPilot/DataPilot && npm run dev",
    "build": "cd DataPilot/DataPilot && npm install && npm run build",
    "start": "cd DataPilot/DataPilot && npm run start",
    "lint": "cd DataPilot/DataPilot && npm run lint",
    "install-deps": "cd DataPilot/DataPilot && npm install",
    "setup": "cd DataPilot/DataPilot && npm install && npx prisma generate",
    "postinstall": "cd DataPilot/DataPilot && npm install"
  },
  "keywords": [
    "mbti",
    "personality",
    "assessment",
    "nextjs",
    "typescript",
    "tailwindcss",
    "psychology",
    "personality-test",
    "react",
    "prisma"
  ],
  "author": "AhmedGabl",
