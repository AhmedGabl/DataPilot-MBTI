  "repository": {
    "type": "git",
    "url": "https://github.com/AhmedGabl/DataPilot-MBTI.git"
  },
  "bugs": {
    "url": "https://github.com/AhmedGabl/DataPilot-MBTI/issues"
  },
  "homepage": "https://github.com/AhmedGabl/DataPilot-MBTI#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}